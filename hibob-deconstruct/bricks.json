{
    "name": "Deconstructed_Distributed_Application_Configuration",
    "description": "Comprehensive configuration for distributed SQS and SNS resources with dynamic environment handling and full variable integration",
    "version": "1.0.1",
    "props": {
        "tags": {
            "type": "map(string)",
            "description": "Standard tags for all resources"
        },
        "organization": {
            "type": "string",
            "description": "The organization this configuration belongs to"
        },
        "environment": {
            "type": "string",
            "description": "The deployment environment (e.g., dev, prod)"
        },
        "region": {
            "type": "string",
            "description": "The AWS region where resources are deployed"
        },
        "account_id": {
            "type": "string",
            "description": "The AWS account ID where resources are deployed"
        },
        "message_retention_seconds": {
            "type": "number",
            "description": "Message retention seconds"
        },
        "visibility_timeout_seconds": {
            "type": "number",
            "description": "Visibility timeout seconds"
        },
        "max_receive_count": {
            "type": "number",
            "description": "Max receive count for redrive policy"
        },
        "dlq_message_retention_seconds": {
            "type": "number",
            "description": "Dead Letter Queue message retention seconds"
        }
    },
    "outs": {
        "hibob-sqs-local-Compensation": {
            "type": "string",
            "description": "Compensation topic",
            "value": "Data.hibob-sqs-local-Compensation.sqs_queue_arns"
        },
        "hibob-sqs-local-Docs": {
            "type": "string",
            "description": "Docs topic",
            "value": "Data.hibob-sqs-local-Docs.sqs_queue_arns"
        },
        "hibob-sqs-local-Mapping": {
            "type": "array",
            "description": "List of ARNs for the SQS queues",
            "value": "Data.hibob-sqs-local-Mapping.sqs_queue_arns"
        }
    },
    "packages": [
        {
            "id": "hibob-sqs-local-Compensation",
            "path": "git::https://github.com/andriizavoiko/tree.git//hibob-sqs-local-Compensation.git?ref=_pov-variables",
            "version": "1.0.0"
        },
        {
            "id": "hibob-sqs-local-Docs",
            "path": "git::https://github.com/andriizavoiko/tree.git//hibob-sqs-local-Docs.git?ref=_pov-variables",
            "version": "1.0.0"
        },
        {
            "id": "hibob-sqs-local-Mapping",
            "path": "git::https://github.com/andriizavoiko/tree.git//hibob-sqs-local-Mapping.git?ref=_pov-variables",
            "version": "1.0.0"
        }
    ]
}